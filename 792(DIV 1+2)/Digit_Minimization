#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
       string s;
       cin>>s;
      long long int mn=999999999,mn1=999999999;
       for(long long int i=0;i<s.size();i++)
       {
           if(s[i]-'0'<mn)mn=s[i]-'0';
       }

       if(s.size()==2)cout<<s[1]<<endl;
       else cout<<mn<<endl;

    }
}
