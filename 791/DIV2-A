

    #include <bits/stdc++.h>
    #define ull unsigned long long
    using namespace std;
     
    void sol() {
    	ull n; cin >> n;
    	if (n%2==1 || n < 4) {cout << -1 << endl; return;}
    	cout << (n%6==0 ? n/6 : n/6 + 1) << " " << n/4 << endl;
    }
     
    int main() {
    	int t; cin >> t;
    	while(t--) {
    		sol();
    	}
    }
